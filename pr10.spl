--PROBLEM 10 PROGRAM

List fibonacci !
fibonacci ++ 1 !
fibonacci ++ 1 !
i = 2 !
while ( i < length(Stream)) then {
fibonacci ++ (fibonacci index i-1) + (fibonacci index i-2) !
i = i + 1 !
}

List outputSequence !

i = 0 !

while ( i < length(Stream)) then {
count = 0 !
sum = 0 !
while ( count < i + 1) then {
sum = sum + (Stream index i - count) * fibonacci index count !
count = count + 1 !
}
outputSequence ++ sum !
i = i + 1 !
}

OUTPUT (outputSequence) !